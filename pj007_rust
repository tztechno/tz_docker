# Dockerfile
FROM rust:latest
WORKDIR /app
# Copy the entire project into the container
COPY . .
# Build the Rust project
RUN cargo build
# Run the compiled Rust program
CMD ["cargo", "run"]


#bash(container)
docker build -t my-python-app .
docker run my-python-app


#bash(host)
cargo init (create Cargo.toml)
cargo run
